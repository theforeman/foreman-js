{
  "name": "@theforeman/vendor",
  "version": "0.1.0-alpha.8",
  "description": "foreman supported 3rd-party node_modules",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/theforeman/foreman-js",
    "directory": "packages/vendor"
  },
  "engines": {
    "node": ">=6"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:dev && npm run build:prod",
    "build:base": "node --max_old_space_size=8192 ./node_modules/.bin/webpack --config webpack.config.js",
    "build:dev": "npm run build:base -- --mode=development",
    "build:prod": "npm run build:base -- --mode=production",
    "analyze": "./scripts/webpack-analyze.sh",
    "lint": "eslint ."
  },
  "main": "dist/manifest.json",
  "files": [
    "dist",
    "lib",
    "scss",
    "webpack.plugin.js"
  ],
  "dependencies": {
    "copy-webpack-plugin": "^4.6.0"
  },
  "devDependencies": {
    "@theforeman/vendor-core": "^0.1.0-alpha.8",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.10.0",
    "eslint-plugin-patternfly-react": "^0.2.2",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-inject-plugin": "^1.1.2",
    "webpack-stats-plugin": "^0.2.1"
  },
  "keywords": []
}
